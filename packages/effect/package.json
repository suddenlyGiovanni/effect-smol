    "typescript",
    "error-handling",
    "concurrency",
    "observability"
  ],
  "keywords": [
    "typescript",
    "error-handling",
    "concurrency",
    "observability"
  ],
  "sideEffects": [],
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./collections": "./src/collections/index.ts",
    "./data": "./src/data/index.ts",
    "./encoding": "./src/encoding/index.ts",
    "./interfaces": "./src/interfaces/index.ts",
    "./optic": "./src/optic/index.ts",
    "./platform": "./src/platform/index.ts",
    "./schema": "./src/schema/index.ts",
    "./stm": "./src/stm/index.ts",
    "./stream": "./src/stream/index.ts",
    "./testing": "./src/testing/index.ts",
    "./types": "./src/types/index.ts",
    "./unstable/ai": "./src/unstable/ai/index.ts",
    "./unstable/cli": "./src/unstable/cli/index.ts",
    "./unstable/cluster": "./src/unstable/cluster/index.ts",
    "./unstable/http": "./src/unstable/http/index.ts",
