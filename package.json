{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "clean": "node scripts/clean.mjs",
    "codegen": "pnpm --recursive --parallel --filter \"./packages/**/*\" run codegen",
    "codemod": "node scripts/codemod.mjs",
    "build": "tspc -b tsconfig.build.json && pnpm --recursive --parallel --filter \"./packages/**/*\" run build",
    "build:tsgo": "tsgo -b tsconfig.build.json && pnpm --recursive --parallel --filter \"./packages/**/*\" run build:tsgo",
    "circular": "node scripts/circular.mjs",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "check": "tspc -b tsconfig.json",
    "check:tsgo": "tsgo -b tsconfig.json",
    "check-recursive": "pnpm --recursive --filter \"./packages/**/*\" exec tspc -b tsconfig.json",
    "lint": "eslint \"**/{src,test,examples,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "pnpm lint --fix",
    "docgen": "pnpm --recursive --filter \"./packages/**/*\" exec docgen && node scripts/docs.mjs",
    "analyze-jsdoc": "node scripts/analyze-jsdoc.mjs",
    "test-types": "tstyche",
    "visualize": "VISUALIZE=true rollup -c rollup.config.js -d tmp",
    "changeset-version": "changeset version",
    "changeset-publish": "pnpm codemod && pnpm build && changeset publish"
  },
  "resolutions": {
    "dependency-tree": "^10.0.9",
    "detective-amd": "^5.0.2",
    "detective-cjs": "^5.0.1",
    "detective-es6": "^4.0.1",
    "detective-less": "^1.0.2",
    "detective-postcss": "^6.1.3",
    "detective-sass": "^5.0.3",
    "detective-scss": "^4.0.3",
    "detective-stylus": "^4.0.0",
    "detective-typescript": "^11.1.0"
  },
  "imports": {
    "#dist/effect": "./packages/effect/dist/index.js",
    "#dist/effect/*": "./packages/effect/dist/*.js",
    "#dist/effect-platform-node": "./packages/platform-node/dist/index.js",
    "#dist/effect-platform-node/*": "./packages/platform-node/dist/*.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@edge-runtime/vm": "^5.0.0",
    "@effect/build-utils": "^0.8.9",
    "@effect/docgen": "https://pkg.pr.new/Effect-TS/docgen/@effect/docgen@49e99a2",
    "@effect/eslint-plugin": "^0.3.2",
    "@effect/language-service": "^0.62.3",
    "@effect/vitest": "workspace:^",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^25.0.2",
    "@typescript/native-preview": "7.0.0-dev.20251215.1",
    "@vitest/browser": "4.0.16",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/expect": "4.0.16",
    "@vitest/web-worker": "4.0.16",
    "ast-types": "^0.14.2",
    "babel-plugin-annotate-pure-calls": "^0.5.0",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "glob": "^13.0.0",
    "happy-dom": "^20.0.11",
    "i18next": "^25.7.3",
    "jscodeshift": "^17.3.0",
    "madge": "^8.0.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.5",
    "rollup-plugin-bundle-stats": "^4.21.7",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.44.1",
    "ts-patch": "^3.3.0",
    "tstyche": "^6.0.0-rc.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "4.0.16",
    "vitest-websocket-mock": "^0.5.0",
    "zod": "^4.2.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "@changesets/assemble-release-plan": "patches/@changesets__assemble-release-plan.patch",
      "@changesets/get-github-info": "patches/@changesets__get-github-info.patch"
    },
    "ignoredBuiltDependencies": [
      "cpu-features",
      "esbuild",
      "lmdb",
      "msgpackr-extract",
      "msw",
      "protobufjs",
      "ssh2",
      "workerd",
      "core-js",
      "unrs-resolver"
    ],
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  }
}
